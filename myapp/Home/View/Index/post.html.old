
<include file="myapp/Home/View/commonheader.html"/>

<body ng-app="myapp">
    <div class="wrap">
        <div class="head">
            <a href="#"><img src="__PUBLIC__/img/index_logo.png" alt="小明的网站"></a>
            <p>小明的网站</p>
        </div>
        <div class="navi">
            <ul>
                <li><a href="#">首页</a></li>
            </ul>
        </div>

	<div class="entrybody">
    <div class="entrytitle">{$data["info"]["title"]}
        <span class="entryinfo">{$data["info"]["createdate"]}</span>
    </div>

<div class="entrycontent" id="entrycontent">
	<foreach name="data['content']" item="item">
		<div class="entryimgs">
	        <a ng-href="{$item["url"]}" rel="lightbox-group"><img src="{$item["url"]}"></a>
	    </div>
	</foreach>


</div>
</div>
<div class="clear"></div>
    </div>



</body>
</html>